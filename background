Frame.Transparency = 1
local tweenService = game:GetService("TweenService")

local function createFadeInTween(object)  -- Function for reuse
    local tweenInfo = TweenInfo.new(
        0.5,       -- Time in seconds
        Enum.EasingStyle.Quad,
        Enum.EasingDirection.Out,
        0,           -- Repeat count
        false,
        0
    )
    return tweenService:Create(object, tweenInfo, {Transparency = 0})
end

local frameTween = createFadeInTween(Frame)
frameTween:Play(1)

local button1Tween = createFadeInTween(Button1)
button1Tween:Play(1)

local button2Tween = createFadeInTween(Button2)
button2Tween:Play(1)
